{
  "name": "website",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production ./bin/www",
    "dev": " nodemon --ignore public/dist ./bin/www --exec babel-node",
    "build": "babel ./src -d dist && NODE_ENV=production node ./bin/build"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "ignore": [
      "node_modules"
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "dependencies": {
    "@sendgrid/mail": "^6.1.3",
    "body-parser": "^1.17.2",
    "contentful": "^4.6.1",
    "express": "^4.15.4",
    "multer": "^1.3.0"
  },
  "devDependencies": {
    "aphrodite": "^1.2.1",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.6.5",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-0": "^6.5.0",
    "babili": "^0.1.3",
    "fs-extra": "^4.0.2",
    "md5": "^2.2.1",
    "mz": "^2.4.0",
    "name-all-modules-plugin": "^1.0.1",
    "nodemon": "^1.11.0",
    "webpack": "^3.6.0",
    "webpack-manifest-plugin": "^1.3.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "vhtml": "^2.1.0"
  }
}
