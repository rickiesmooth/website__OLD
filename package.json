{
  "name": "website",
  "version": "1.0.1",
  "main": "dist/app.js",
  "scripts": {
    "start": "NODE_ENV=production ./dist/bin/www",
    "dev": " nodemon --ignore public/dist ./bin/www --exec babel-node",
    "postinstall": "npm run build",
    "build": "babel . --ignore node_modules,public,dist -d dist && cp -R public dist/public && cp -R ./bin dist/bin"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "ignore": [
      "node_modules"
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "dependencies": {
    "aphrodite": "^1.2.1",
    "contentful": "^4.6.1",
    "express": "^4.15.4",
    "multer": "^1.3.0",
    "fs-extra": "^4.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.6.5",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-0": "^6.5.0",
    "name-all-modules-plugin": "^1.0.1",
    "nodemon": "^1.11.0",
    "webpack": "^3.6.0",
    "webpack-manifest-plugin": "^1.3.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "vhtml": "^2.1.0"
  }
}
