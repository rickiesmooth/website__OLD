{
  "name": "website",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "./bin/www",
    "prestart": "npm run build",
    "dev": "DEV_ENVIRONMENT=true nodemon --ignore public ./bin/www --exec babel-node",
    "build": "babel ./src -d dist && node dist/build && babili ./public/js -d ./public/dist/js/es && babel ./public/js -d ./public/dist/js"
  },
  "meta": [
    "SENDGRID_API_KEY",
    "CONTENTFUL_API_KEY"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "ignore": [
      "node_modules"
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "dependencies": {
    "@sendgrid/mail": "^6.1.3",
    "aphrodite": "^1.2.1",
    "body-parser": "^1.17.2",
    "contentful": "^4.6.1",
    "express": "^4.15.4",
    "multer": "^1.3.0",
    "mz": "^2.4.0",
    "rimraf": "^2.6.2",
    "vhtml": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.6.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babili": "^0.1.3",
    "nodemon": "^1.11.0"
  }
}
